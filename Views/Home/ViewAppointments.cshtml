@{
    ViewBag.Title = "Scheduled Appointments";

}
@model GroupInfo

<div>
    <h2>Here are the submitted appointments.</h2>   
    <table  class="table table-striped">
        <thead>
            <tr>
                <th>Scheduled Time</th>
                <th>Group Name</th>
                <th>Group Size</th>
                <th>Email Address</th>
                <th>Phone Number</th>

            </tr>
        </thead>
        <tbody>

          
                <tr>
                    <td>@Model.TimeSlot</td>
                    <td>@Model.Name</td>
                    <td>@Model.Size</td>
                    <td>@Model.Email</td>
                    <td>@Model.PhoneNumber</td>

                </tr>
            

        </tbody>
    </table>
</div>